<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Colors-->
    <Color x:Key="ColorPrimary1">#FFBA33</Color>
    <Color x:Key="ColorPrimary2">#393939</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="Brush1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="Brush2" Color="{StaticResource ColorPrimary2}"/>
</ResourceDictionary>